@model Time
@{
    ViewData["Title"] = "Cadastrar";
}



    <marquee><h1>Cadastrar</h1></marquee>

    <form asp-action="Time" asp-controller="Cadastrar">
        <div class="form-group">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="DataFundacao"></label>
            <input asp-for="DataFundacao" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="FolhaPagamento"></label>
            <input asp-for="FolhaPagamento" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Vitorias"></label>
            <input asp-for="Vitorias" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Classificado"></label>
            <input asp-for="Classificado" />
        </div>

        <input type="submit" name="cadastrar" value="Cadastrar" />
        

      
    </form>