@model Corrida
@{
    ViewData["Title"] = "Cadastrar";
}

<h1>Cadastrar</h1>

<form asp-action="Cadastrar" asp-controller="corrida" >
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div>
        <label asp-for="Distancia"></label>
        <input asp-for="Distancia" class="form-control" />
    </div>
    <div>
        <label asp-for="Premiacao"></label>
        <input asp-for="Premiacao" class="form-check" />
    </div>
<input type="submit" value="Cadastrar"/>
</form>